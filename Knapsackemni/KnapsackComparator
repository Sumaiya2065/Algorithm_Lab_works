package Knapsack.emni;

import java.util.*;

public class KnapsackComparator implements Comparator<Knapsack> {

	@Override
	public int compare(Knapsack o1, Knapsack o2) {
		// TODO Auto-generated method stub
		double cmp1=o1.getProfit()/o1.getWeight();
		double cmp2=o2.getProfit()/o2.getWeight();
		if(cmp1<cmp2) {
			return 1;
		}
		else {
			return -1;
		}
	
		
	}
	
}
