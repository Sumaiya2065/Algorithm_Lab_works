#include<bits/stdc++.h>
using namespace std;

int main( ){

   int n;
   cout<<"enter valu: "<<endl;
   cin>>n;
   int ar[n];
   cout<<"enter array elements: "<<endl;

   for(int i=0;i<n;i++){
      cin>>ar[i];

   }
   for( int i=0;i<n;i++){
      for( int j=0;j<n-1-i;j++){
         if(ar[j]>ar[j+1]){
            swap(ar[j],ar[j+1]);
         }
      }
   }
   for(int i=0;i<n;i++){
      cout<<ar[i]<<"  ";
   }







}
